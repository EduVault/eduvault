{
  "name": "@eduvault/api",
  "version": "1.0.0",
  "description": "eduvault auth server",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist yarn.lock node_modules",
    "clean:dist": "rimraf dist",
    "mongo-start": "brew services run mongodb-community",
    "mongo-status": "brew services list",
    "mongo-stop": "brew services stop mongodb-community",
    "dev:watch": "TS_NODE_PROJECT='./tsconfig.build.json' ts-node-dev --inspect --poll --respawn ./src src/index.ts",
    "dev": "run-s dev:watch",
    "start": "node dist/",
    "watch": "tsc -p tsconfig.build.json -w ",
    "build:server": "tsc -p tsconfig.build.json ",
    "build": "run-s clean:dist build:server",
    "test": "env TEST=true jest --env=./jest-env.js --detectOpenHandles",
    "test:watch": "env TEST=true jest --env=./jest-env.js --detectOpenHandles  --watch"
  },
  "author": "Jacob Cohen-Rosenthal",
  "license": "MIT",
  "dependencies": {
    "@eduvault/shared": "^1.1.25",
    "@koa/cors": "^3.1.0",
    "@textile/hub": "^6.0.2",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "core-js-bundle": "^3.8.1",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "emittery": "^0.8.0",
    "google-protobuf": "^3.2.0",
    "ip": "^1.1.5",
    "isomorphic-ws": "^4.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-cookie": "^1.0.0",
    "koa-csrf": "^3.0.8",
    "koa-helmet": "^6.0.0",
    "koa-logger": "^3.2.1",
    "koa-passport": "^4.1.3",
    "koa-response2": "^1.0.4",
    "koa-router": "^10.0.0",
    "koa-session": "^6.0.0",
    "koa-sslify": "^4.0.3",
    "koa-websocket": "^6.0.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.1",
    "openapi-types": "^7.0.1",
    "openapi-validator-middleware": "^3.0.0",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/crypto-js": "^4.0.1",
    "@types/glob": "^7.1.3",
    "@types/ip": "^1.1.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-cookie": "^1.0.0",
    "@types/koa-helmet": "^6.0.1",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-router": "^7.4.1",
    "@types/koa-session": "^5.10.2",
    "@types/koa-sslify": "^4.0.1",
    "@types/koa-websocket": "^5.0.4",
    "@types/koa__cors": "^3.0.1",
    "@types/lodash": "^4.14.153",
    "@types/mongoose": "^5.7.29",
    "@types/node": "^14.0.9",
