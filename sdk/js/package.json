    "buffer": "^6.0.3",
    "core-js": "^3.8.3",
    "crypto-js": "^4.0.0",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.1",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "cspell": "^4.1.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.2"
