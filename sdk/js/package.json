  "description": "asdf",
  "main": "dist/main/index.js",
  "typings": "dist/main/index.d.ts",
  "module": "dist/module/index.js",
  "repository": "https://github.com/Jewcub/eduvault-test",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "clean": "rimraf dist node_modules yarn.lock",
    "inst": "yarn add @eduvault/shared && yarn install",
    "prepublishOnly": "run-s clean inst build",
    "build": "run-p build:*",
    "build:main": "tsc -p  tsconfig.json",
    "build:module": "tsc -p  tsconfig.module.json",
    "watch:module": "tsc -p  tsconfig.module.json -w",
    "watch:build": "tsc -p  tsconfig.json -w",
    "dev": "run-p watch:module watch:build"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@eduvault/shared": "^1.0.16",
    "@textile/hub": "^5.2.0",
